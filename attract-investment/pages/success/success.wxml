<!--success.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">操作成功</view>
            <view class="weui-msg__desc">恭喜您，数据提交成功</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
              <navigator url="{{url}}" redirect="true">
                <button class="weui-btn" type="primary">完成</button>
              </navigator>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__text">Copyright © 2008-2016 bioppp.com</view>
            </view>
        </view>
    </view>
</view>